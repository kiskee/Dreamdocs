{
  "name": "dreamdocs",
  "displayName": "Dreamdocs",
  "description": "ðŸš€ AI-powered code documentation and refactoring tool. Generate JSDoc comments, inline explanations, and improve naming with Groq AI. Boost your code quality!",
  "version": "1.2.0",
  "publisher": "DanielMeCoss",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiskee/Commentify.git"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other", "Snippets", "Formatters"],
  "keywords": ["jsdoc", "comments", "documentation", "ai", "groq", "typescript", "javascript", "code-generation", "refactoring", "naming"],
  "icon": "icon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dreamdocs.generateJSDoc",
        "title": "Generate JSDoc Comment"
      },
      {
        "command": "dreamdocs.generateInline",
        "title": "Generate Inline Comment"
      },
      {
        "command": "dreamdocs.refactorNaming",
        "title": "Refactor Naming"
      },
      {
        "command": "dreamdocs.setApiKey",
        "title": "Set Groq API Key"
      },
      {
        "command": "dreamdocs.testConnection",
        "title": "Test Groq Connection"
      },
      {
        "command": "dreamdocs.addDebugLogs",
        "title": "Add Smart Debug Logs"
      },
      {
        "command": "dreamdocs.removeDebugLogs",
        "title": "Remove Debug Logs"
      }
    ],
    "keybindings": [
      {
        "command": "dreamdocs.generateJSDoc",
        "key": "ctrl+shift+d",
        "when": "editorTextFocus"
      },
      {
        "command": "dreamdocs.generateInline",
        "key": "ctrl+shift+c",
        "when": "editorTextFocus"
      },
      {
        "command": "dreamdocs.refactorNaming",
        "key": "ctrl+shift+n",
        "when": "editorTextFocus"
      },
      {
        "command": "dreamdocs.addDebugLogs",
        "key": "ctrl+shift+l",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "groq-sdk": "^0.3.3"
  }
}